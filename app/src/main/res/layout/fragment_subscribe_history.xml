<?xml version="1.0" encoding="utf-8"?>
<com.tpyzq.mobile.pangu.view.pullDownGroup.PullDownScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/XLSX"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size5"
        android:background="@color/bgColor" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/lvSubscribeHistory"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="#00000000"
            android:dividerHeight="0dip" />

        <ImageView
            android:id="@+id/ivSubHistoryKong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="@dimen/size100"
            android:src="@mipmap/kong"
            android:visibility="gone" />

    </RelativeLayout>


</com.tpyzq.mobile.pangu.view.pullDownGroup.PullDownScrollView>
